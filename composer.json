{
    "name": "symfony2admingenerator/form-bundle",
    "type": "symfony-bundle",
    "description": "Twig extension introduceing new blocks to seperate form HTML structure from CSS decorating it and JS enhancing it's functionalities.",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Gołębiewski",
            "email": "loostro@gmail.com",
            "role": "Symfony2 bundle author"
        },
        {
            "name": "Symfony community",
            "homepage": "https://github.com/symfony2admingenerator/FormBundle/graphs/contributors"
        }
    ],
    "require": {
        "symfony/symfony": ">= 2.2.0",
        "twig/twig": ">= 1.9.0",
        "twig/extensions": "1.0.*"
    },
    "suggest": {
        "cedriclombardot/admingenerator-generator-bundle": "The missing symfony2 admin generator"
    },
    "autoload": {
        "psr-0": {
            "Admingenerator\\FormBundle": ""
        }
    },
    "target-dir": "Admingenerator/FormBundle"
}
